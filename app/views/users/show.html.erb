<% flash.each do |type, msg| %>
    <div>
      <%= msg %>
    </div>
  <% end %>

<% if @user.admin %>
    ADMIN
<% end %>
<%= @user.name %>
<% if !@user.admin %>
    <p>Height: <%= @user.height %></p>
    <p>Happiness: <%= @user.happiness %></p>
    <p>Nausea: <%= @user.nausea %></p>
    <p>Tickets: <%= @user.tickets %></p>
    <p>Mood: <%= @user.mood %></p>
 <% end %>

